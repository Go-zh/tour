[project]
	description = The Go Tour (Chinese version)
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Minux Ma (minux)
[access "refs/for/refs/*"]
	read = group user/Minux Ma (minux)
	push = group user/Minux Ma (minux)
	label-Code-Review = -2..+2 group user/Minux Ma (minux)
	label-Verified = -1..+1 group user/Minux Ma (minux)
	rebase = group user/Minux Ma (minux)
	removeReviewer = group user/Minux Ma (minux)
	submit = group user/Minux Ma (minux)
[access "refs/heads/*"]
	read = group user/Minux Ma (minux)
	create = group user/Minux Ma (minux)
	push = +force group user/Minux Ma (minux)
	pushMerge = group user/Minux Ma (minux)
[access "refs/tags/*"]
	push = +force group user/Minux Ma (minux)
	pushTag = group user/Minux Ma (minux)
	pushSignedTag = group user/Minux Ma (minux)
[submit]
	action = cherry pick
